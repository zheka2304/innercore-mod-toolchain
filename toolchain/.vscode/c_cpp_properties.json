{
    "configurations": [
        {
            "name": "Inner Core ARM64",
            "compilerPath": "${workspaceFolder}/toolchain/ndk/arm64/bin/clang90++.exe",
            "includePath": [
                "${workspaceFolder}/toolchain/stdincludes/gnu_stl",
                "${workspaceFolder}/toolchain/stdincludes/horizon",
                "${workspaceFolder}/toolchain/ndk/arm64/sysroot/usr/include",
                "${workspaceFolder}/toolchain/ndk/arm64/sysroot/usr/include/linux",
                "${workspaceFolder}/toolchain/ndk/arm64/sysroot/usr/include/c++/v1"
            ],
            "cppStandard": "c++17",
            "cStandard": "c11",
            "intelliSenseMode": "linux-clang-arm64",
            "compilerArgs": [
                "--target=aarch64-linux-androideabi24",
                "-DANDROID_STL=c++_static",
                "-DARM64=1"
            ]
        },
        {
            "name": "Inner Core ARM32",
            "compilerPath": "${workspaceFolder}/toolchain/ndk/arm/bin/arm-linux-androideabi-g++.exe",
            "includePath": [
                "${workspaceFolder}/toolchain/stdincludes/gnu_stl",
                "${workspaceFolder}/toolchain/stdincludes/horizon",
                "${workspaceFolder}/toolchain/ndk/arm/sysroot/usr/include",
                "${workspaceFolder}/toolchain/ndk/arm/include/c++/4.9.x/tr1",
                "${workspaceFolder}/toolchain/ndk/arm/include/c++/4.9.x",
                "${workspaceFolder}/toolchain/ndk/arm/lib/gcc/arm-linux-androideabi/4.9.x/include",
                "${workspaceFolder}/toolchain/ndk/arm/include/c++/4.9.x/arm-linux-androideabi"
            ],
            "cppStandard": "c++11",
            "cStandard": "c11",
            "intelliSenseMode": "linux-clang-arm"
        }
    ],
    "version": 4
}
